<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.navigator.repository.RoutePointRepository">

    <insert id="saveRoutePoint">
        INSERT INTO route_point (route_id, point_id, order_index)
        VALUES (#{routeId}, #{pointId}, #{orderIndex});
    </insert>

</mapper>